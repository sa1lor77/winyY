{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }

  .image-product-tab .tablinks .tabs_container .dt-sc-tabs-content-Details{opacity:0;transition:all 0.3s linear;display:block;padding: 0;margin: 0;}
  .image-product-tab .tablinks.active:hover .tabs_container .dt-sc-tabs-content-Details {opacity:1;}
   .image-product-tab .tablinks.active .tabs_container .dt-sc-tabs-content-Details.active{opacity:1;}
  .image-product-tab  .tabs .tablinks:hover,  .image-product-tab .tabs .tablinks.active{box-shadow:none}
  .image-product-tab.product-tab-wrapper .collection .grid>.grid__item{ max-width: calc(36% - 50px);width: calc(36% - 50px);}
  .image-product-tab.product-tab-wrapper .collection .grid>.grid__item[class*=tab-template--]:not(:only-child){
    max-width: calc(64% - 50px);
    width: calc(64% - 50px);
    display: flex;
    align-items: center;
   }
    
  .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs{display:block;margin:0;position: relative; background: transparent;width: 100%;max-width: 100%;}
  .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs .tablinks{justify-content: space-between;padding:56px 0;margin:0;transition:all 0.3s linear;background:transparent;border:none;box-shadow: none;text-transform:uppercase;}
  .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs .tablinks:not(:last-child){margin-bottom: 40px;}
  .image-product-tab.product-tab-wrapper .tabs_container {
    position: absolute;
    width: auto;
    height: auto;
    right: 0;
    top: 0%;
    z-index: 1;transform: translateY(0%);-webkit-transform: translateY(0%);}
    .image-product-tab.product-tab-wrapper .collection .grid{grid-gap:0;padding: 75px 0;position:relative; }
    .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs .tablinks a.tab_title{font-size: 38px;line-height: normal;font-weight: 400;position:relative;letter-spacing: 5px;width: 370px;text-align: center;}
    .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs .tablinks:hover a.tab_title{color: rgba(var(--color-base-solid-button-labels));}
    .image-product-tab.product-tab-wrapper .tabs .tablinks img{margin:0;border-radius: 10px; display: block;}
    .image-product-tab.product-tab-wrapper .collection .tabs .tablinks:not(:last-child){margin:0;}

   .image-product-tab.product-tab-wrapper .collection__view-all{margin-top:100px;}
   .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs .tablinks .image-text{
    margin: 10px 0 0;
    font-size: 12px;
    text-align: center;
    color: rgba(var(--color-base-solid-button-labels));
    font-family: var(--font-body-family);
    font-weight: 600;
    letter-spacing: 2.6px;
    }
    .image-product-tab.section-{{ section.id }}:after{content:''; {% if section.settings.after_bg_image != blank %} background:url('{{ section.settings.after_bg_image | image_url: width: 1920  }}'); {% else %} background: url('{{ 'background.svg' | asset_url }}'); background-size: cover; background-position: center; {% endif %} top:0;left:0;width:100%;height:100%;background-repeat:no-repeat;background-size:cover;position:absolute;z-index:0;}
    .image-product-tab.section-{{ section.id }}{position:relative;}
    .image-product-tab.product-tab-wrapper .collection .grid__item>.media{border-radius: var(--card-corner-radius);height:auto;}
    .image-product-tab.product-tab-wrapper .collection .grid__item>.media>img{min-height:auto;height:auto;}
 
    .image-product-tab.product-tab-wrapper .collection .tabs_container{position: absolute; width: auto;height: auto;pointer-events: none;transform: rotate(9.5deg);  border-radius: 10px;}
    .image-product-tab.product-tab-wrapper .collection .tabs .tablinks.active{color: rgb(var(--color-base-solid-button-labels));}
    .image-product-tab.product-tab-wrapper .collection  .tablinks:last-child  .tabs_container{top:auto;bottom:-30px;}
    .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs .tablinks:first-child{padding:0 0 56px;}
    .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs .tablinks:last-child{padding:56px 0 0 ;}
    .image-product-tab.product-tab-wrapper .collection .grid>.grid__item{overflow:visible;}
    .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs .tablinks:after{content:'';width:100%;height:1px;background:rgba(var(--color-base-accent-1), 0.3);display:block;left:0;position:absolute;opacity:1;z-index:-1;bottom:0;}
    .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs .tablinks:last-child:after{display:none;}
    .image-product-tab.product-tab-wrapper .collection .tabs .tablinks.active:after,
    .image-product-tab.product-tab-wrapper .collection .tabs .tablinks:hover:after{width:100%;}
    .image-product-tab.product-tab-wrapper .image-with-text__grid:after{content:'';width:100%;height:1px;background:rgba(var(--color-base-accent-1), 0.3);top: 0;
    position: absolute;
    left: 0;}
    .image-product-tab.product-tab-wrapper .image-with-text__grid:before{content:'';width:100%;height:1px;background:rgba(var(--color-base-accent-1), 0.3);bottom: 0;
    position: absolute;
    left: 0;}

  @media screen and (min-width: 1541px) {
    .image-product-tab.product-tab-wrapper .image-with-text__grid:after{width:1815px;left: -92px;}
    .image-product-tab.product-tab-wrapper .image-with-text__grid:before{width:1815px;left: -92px;}
      .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs .tablinks:after{width:1815px;left:-730px;}
    .image-product-tab.product-tab-wrapper .collection .tabs .tablinks.active:after,
    .image-product-tab.product-tab-wrapper .collection .tabs .tablinks:hover:after{width:1815px;}
  }
  
  @media screen and (max-width: 1540px) {
  .image-product-tab.product-tab-wrapper .collection .grid>.grid__item{ max-width: calc(35% - 30px);width: calc(35% - 30px);margin-bottom:0;}
  .image-product-tab.product-tab-wrapper .collection .grid>.grid__item[class*=tab-template--]:not(:only-child){max-width: calc(65% - 30px);width: calc(65% - 30px);margin-bottom:0;}  
   .image-product-tab.product-tab-wrapper .collection__view-all{margin-top:50px;} 
    .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs .tablinks{padding:40px 0;}
    .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs .tablinks:first-child {padding: 0 0 40px;}
    .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs .tablinks:last-child { padding: 40px 0 0;}
    .image-product-tab.product-tab-wrapper .collection .grid{padding:40px 0;}
    .image-product-tab.product-tab-wrapper .tabs .tablinks img{width:350px;}
  }
 @media screen and (max-width: 1439px) {
   .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs .tablinks a.tab_title{font-size:36px;width:300px;}
 }
 @media screen and (max-width: 1199px) {
   .image-product-tab.product-tab-wrapper .collection .grid>.grid__item{ max-width: calc(35% - 15px);width: calc(35% - 15px);margin-bottom:0;}
  .image-product-tab.product-tab-wrapper .collection .grid>.grid__item[class*=tab-template--]:not(:only-child){max-width: calc(65% - 15px);width: calc(65% - 15px);margin-bottom:0;}  
   .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs .tablinks a.tab_title{width:260px;letter-spacing:3px;}
 }

 @media screen and (max-width: 990px){
.image-product-tab.product-tab-wrapper .collection .grid>.grid__item, .image-product-tab.product-tab-wrapper .collection .grid>.grid__item[class*=tab-template--]:not(:only-child) {
    max-width: 100%;
    width: 100%;
}   
   .image-product-tab.product-tab-wrapper .collection .grid{flex-direction:column;}
   .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs{    margin-top: 40px;}
   .image-product-tab.product-tab-wrapper .tabs_container{top: 0;}
   .image-product-tab.product-tab-wrapper .tablinks:last-child .tabs_container {bottom: -35px;top: auto;}
  }
   @media screen and (max-width: 767px){
  .image-product-tab.product-tab-wrapper .tabs .tablinks img { width: 280px;}
   }
   @media screen and (max-width: 576px){
  .image-product-tab.product-tab-wrapper .collection .tabs .tablinks.active{background:transparent;}
  .image-product-tab.product-tab-wrapper .collection .tabs .tablinks:not(.active){position:relative;opacity:1;width:100%;}
   .image-product-tab.product-tab-wrapper .collection .tabs :is(.tablinks.active):after{  content: '';
    width: 100%;
    height: 1px;
    background: rgba(var(--color-base-accent-1), 0.3);
    display: block;
    left: 0;
    position: absolute;
    opacity: 1;
    z-index: -1;top:unset;
    bottom: 0; transform: none;-webkit-mask-image: none;right:0;
} 
  .image-product-tab.product-tab-wrapper .collection .tabs :is(.tablinks.active:last-child):after{
   opacity: 0;
  }
    .image-product-tab.product-tab-wrapper .collection .tabs:not(.expanded) .tablinks:not(.active){pointer-events: auto;} 
    .image-product-tab.product-tab-wrapper .tabs .tablinks img { width: 250px;} 
     .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs .tablinks a.tab_title{font-size:24px;text-align:left;}

   }
   @media screen and (max-width: 479px){
   .image-product-tab .tablinks .tabs_container .dt-sc-tabs-content-Details a img{width:150px;}
   .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs .tablinks a.tab_title {font-size: 24px;} 
   .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs .tablinks .image-text{letter-spacing:1px;font-size:10px;}  
   }
     @media screen and (max-width: 380px){
     .image-product-tab .tablinks .tabs_container .dt-sc-tabs-content-Details a img {width: 120px;}
     .image-product-tab.product-tab-wrapper .image-with-text__grid .tabs .tablinks a.tab_title {font-size: 22px;letter-spacing: 1px;}  
     }
    .placeholder-image *{ position: static; }
  .image-product-tab.product-tab-wrapper .tabs .tablinks svg{
    max-width: 300px;
    
  }
{%- endstyle -%}

{% liquid
        case section.settings.products_per_row
        when 1
        assign column = 'one-column'
        when 2
        assign column = 'two-column'
        when 3
        assign column = 'three-column'
        when 4
        assign column = 'four-column'
        when 5
        assign column = 'five-column'
        when 6
        assign column = 'six-column'
        endcase
        %}
  
<div class="product-tab-wrapper image-product-tab section-{{ section.id }} {{ section.settings.custom_class_name }} color-{{ section.settings.color_scheme }} gradient">
  <div class="{% if section.settings.page_full_width %} page-full-width {% else %} page-width {% endif %} {% if section.settings.page_full_width_spacing %} page-full-width_spacing {% endif %} section-{{ section.id }}-padding isolate">
    <div class="row">
  <div class="collection">
     {%- unless section.settings.title == blank -%}
      <div class="title-wrapper-with-link title-wrapper--self-padded-mobile title-wrapper--no-top-margin text-align-{{ section.settings.column_alignment }}">
        <h6 class="sub-heading">
          {{ section.settings.text | escape }}
         </h6>
        <h2 class="title {{ section.settings.heading_size }}">
          {{ section.settings.title | escape }}
        </h2>
        {%- if section.settings.button_label != blank and show_mobile_slider -%}
          <a href="{{ section.settings.button_link }}" class="link underlined-link large-up-hide">{{ section.settings.button_label | escape }}</a>
        {%- endif -%}
      </div>
    {%- endunless -%}
    <div class="image-with-text__grid grid{% if section.settings.layout == 'text_first' %} image-with-text__grid--reverse{% endif %}">
      {%- unless section.settings.layout == 'none' -%}
      <div class="grid__item">
         <div class="image-with-text__media media image-with-text__media--placeholder placeholder"
             {% if section.settings.height == 'adapt' and section.settings.image != blank %}
             style="padding-bottom: {{ 1 | divided_by: section.settings.image.aspect_ratio | times: 100 }}%;"
             {% endif %}
             >
          {%- if section.settings.image != blank -%}
          <img 
               src="{{ section.settings.image | image_url: width: 533 }}"
               alt="{{ section.settings.image.alt | escape }}"
               loading="lazy"
               >
          {%- else -%}
            <div class="placeholder-image " style="position: static; height: 700px;">
          {{ 'hero-apparel-3' | placeholder_svg_tag: 'placeholder-svg' }}
            </div>
          {%- endif -%}
          {% if section.settings.image_block_heading != blank or section.settings.image_block_sub_heading != blank or section.settings.image_block_description != blank or section.settings.image_block_button_link != blank %}
          <div class="image-block-heading color-{{ section.settings.image_color_scheme }} {{ section.settings.image_block_vertical_position | escape }} {{ section.settings.image_block_horizontal_position | escape }}
                      {%- if section.settings.image_block_enable_overlay -%} image-block-overlay {%- endif -%}
                      ">
            {% if section.settings.image_block_sub_heading != blank %}
            <h4 class="image-block-sub-heading">{{ section.settings.image_block_sub_heading }}</h4>
            {% endif %}
            {% if section.settings.image_block_heading != blank %}
            <h3 class="image-block-main-heading">{{ section.settings.image_block_heading }}</h3>
            {% endif %}
            {% if section.settings.image_block_description != blank %}
            <p class="image-block-heading-description">{{ section.settings.image_block_description }}</p>
            {% endif %}
            {% if section.settings.image_block_button_link != blank and section.settings.image_block_button_text != blank %}
            <a href="{{ section.settings.image_block_button_link }}" class="button button--primary">{{ section.settings.image_block_button_text }}</a>
            {% endif %}
          </div>
          {% endif %}
         </div>
      </div>
      {%- endunless -%}  
      <div class="grid__item tab-{{ section.id }}-js" id="tab-{{ section.id }}-js" data-tab-wrapper="tab-main-{{ section.id }}">
        

        <div class="tabs">
          {% for block in section.blocks %}    
          <div class="tablinks"  id="dT_tab-{{ forloop.index }}">
            {%- if section.settings.enable_tab_title != blank -%}
            {% unless block.settings.title == blank %} <a href="{{ block.settings.collection_url }}" class="tab_title full-unstyled-link" >{{ block.settings.title }}</a>{% endunless %}
            {%- endif -%}
             <div class="tabs_container">
           <ul class=" dt-sc-tabs-content-Details" data-tab="dT_tab-{{ forloop.index }}">
            {%- if section.settings.enable_collection_icon != blank -%}
                <a href="{{ block.settings.collection_url }}" class="full-unstyled-link">
            {%- assign placeholder_image_index = forloop.index0 | modulo: 4 | plus: 1 -%}
            {%- assign placeholder_image = 'collection-apparel-' | append: placeholder_image_index -%}
          {% if block.settings.collection_image != blank %}
           <img 
               src="{{ block.settings.collection_image | image_url: width: 533 }}"
               alt="{{ block.settings.collection_image.alt | escape }}"
               loading="lazy"
               >
            {% else %}
             {{ placeholder_image | placeholder_svg_tag: 'placeholder-svg' }}
          {% endif %}
               </a>   
            {%- endif -%}
             {%- if block.settings.image_text != blank -%}
              <p class="image-text">{{ block.settings.image_text }}</p>
            {%- endif -%}    
          </ul>
        
         
   </div> 
          </div>   
          {% endfor %}
          
        </div>

      {%- if section.settings.show_view_all  -%}
      <div class="left collection__view-all">
        <a href="{{ routes.all_products_collection_url }}"
          class="button "
          aria-label="{{ 'sections.featured_collection.view_all_label' | t: collection_name: section.settings.collection.title }}"
        >
          {{ 'sections.featured_collection.view_all' | t }}
        </a>
      </div>
    {%- endif -%}

  </div>  
      
</div>
</div>
</div>
{%- style -%}

